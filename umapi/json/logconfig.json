{
    "version": 1,
    "disable_existing_loggers": false,
	"formatters": {
		"simple": {
			"format": "%(levelname)-8s %(message)s"
		},
		"detailed": {
			"format": "%(asctime)s %(module)s %(threadName)s %(levelname)-8s %(message)s"
		}
	},
	"handlers": {
		"console": {
			"class": "logging.StreamHandler",
			"level": "INFO",
			"formatter": "simple"
		},
		"file": {
			"class": "logging.FileHandler",
			"level": "DEBUG",
			"formatter": "detailed",
			"filename": null
		}
	},
	"loggers": {
		"main": {
			"handlers": [],
			"level": null,
            "propagate": false
		}
	}
}